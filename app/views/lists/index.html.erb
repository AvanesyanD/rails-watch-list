<div class="container mt-2">
<h1>Movie Category</h1>

<ul class= "list-group">
  <% @lists.each do |list| %>
    <li class="list-group-item"><%= link_to list.name, list_path(list), class: "text-decoration-none text-secondary" %></li>
  <% end %>
</ul>

<%= link_to "Add List", new_list_path, class: "btn btn-secondary btn-lg mt-3" %>
</div>


